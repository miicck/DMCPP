dimensions	   1       # The dimensionallity of the system
walkers		   1000    # The number of DMC walkers
iterations	   10000   # The number of DMC iterations to carry out 
tau		   0.01    # The DMC timestep
#write_wavefunction false   # Dont write wavefunction files
tau_c_ratio        10.0   # Ratio of tau_c to tau

# Create a harmonic well with two non interacting fermions in it
harmonic_well 1
particle non_interacting_fermion 0 0 1 0
particle non_interacting_fermion 0 0 1 0
