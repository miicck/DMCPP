dimensions	   3     # The dimensionallity of the system
walkers		   1000  # The number of DMC walkers
iterations	   10000 # The number of DMC iterations to carry out 
tau		   0.01  # The DMC timestep
#no_wavefunction          # Dont write wavefunction files

# Create a Hydrogen atom
atomic_potential 1 0 0 0
particle electron 0 -1 1 0 0 0
